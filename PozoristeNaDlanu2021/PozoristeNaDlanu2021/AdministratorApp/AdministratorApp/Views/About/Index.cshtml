@model AdministratorApp.Models.Informacije
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/_LayoutPage1.cshtml";
}

<!--/breadcrumbs -->
<div class="w3l-breadcrumbs">
    <nav id="breadcrumbs" class="breadcrumbs">
        <div class="container page-wrapper">
            <a href="/Home/Index">Home</a> »  <span class="breadcrumb_last" aria-current="page">About</span>
        </div>
    </nav>
</div>
<h3>
    @Html.ActionLink("Dodaj nove podatke o pozoristu...", "CreateInfo")
</h3>
<!--//breadcrumbs -->
<!-- /about-->
<div class="w3l-ab-grids py-5">
    <div class="container py-lg-4">
        <div class="row ab-grids-sec align-items-center">
            <div class="col-lg-4 align-items-md-center">
                <img class="img-fluid" src="data:image/jpeg;base64,">
            </div>
            @foreach (var item in Model.informacije)
            {
        <div class="col-lg-8 pl-lg-4 mt-lg-0 mt-5">
            <h3 class="hny-title">@item.naslov</h3>
            <p class="mt-3">
                @item.sadrzaj
            </p>
            @Html.ActionLink("Edit", "EditInfo", new { id = item.id }) |
            @Html.ActionLink("Delete", "DeleteInfo", new { id = item.id })

        </div>
            }
        </div>
    </div>
</div>
<!-- //about-->
<!--grids-sec1-->
<h3>
    @Html.ActionLink("Dodaj novog glumca u pozoristu...", "CreateGlumac")
</h3>
<section class="w3l-team" id="team">
    <div class="grids-main py-5">
        <div class="container py-lg-4">
            <div class="headerhny-title">
                <h3 class="hny-title">Glumci</h3>
            </div>
            <div class="owl-team owl-carousel owl-theme">

                @foreach (var item in Model.glumci)
                {
                    <div class="item vhny-grid">
                        <div class="box16">
                            <div class="d-grid team-info">
                                <div class="column position-relative">
                                    <a href="/About/GlumacDetails/@item.id"><img src="@Url.Content(item.slikeGlumca.FirstOrDefault())" alt="" class="img-fluid rounded team-image" /></a>
                                </div>
                                <div class="column text-center">
                                    <h3 class="name-pos"><a href="/About/GlumacDetails/@item.id">@item.ime @item.prezime</a></h3>

                                </div>
                            </div>
                        </div>

                    </div>
                }




            </div>
        </div>



    </div>
</section>
